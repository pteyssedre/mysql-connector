{
  "name": "mysqlconnector",
  "version": "2.0.1",
  "description": "MySQL connector",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "test": "nyc --reporter=html --reporter=text mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/pteyssedre/mysql-connector.git"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "coverage",
      ".nyc_output",
      "index.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "keywords": [
    "mysql",
    "connector",
    "typescript",
    "async",
    "await",
    "transaction",
    "dbcontext"
  ],
  "author": {
    "name": "Pierre Teyssedre",
    "email": "pierre@teyssedre.ca"
  },
  "license": "MIT",
  "dependencies": {
    "mysql": "2.18.1"
  },
  "devDependencies": {
    "@types/chai": "4.3.1",
    "@types/mocha": "9.1.1",
    "@types/mysql": "2.15.21",
    "chai": "4.3.6",
    "coveralls": "^3.1.1",
    "mocha": "10.0.0",
    "nyc": "^15.1.0",
    "ts-node": "10.7.0",
    "tslint": "^6.1.3",
    "typescript": "4.6.4"
  }
}
